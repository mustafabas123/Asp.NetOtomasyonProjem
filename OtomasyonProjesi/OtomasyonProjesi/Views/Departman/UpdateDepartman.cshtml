@model OtomasyonProjesi.Models.Classlar.Department

@{
    ViewBag.Title = "UpdateDepartman";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    @Html.HiddenFor(x =>x.DepartmentId)
    @Html.Label("Departman Adı")
    @Html.TextBoxFor(x =>x .DepartmentName,new {@class="form-control" })
    <br />
    <button class="btn btn-outline-success">Güncelle</button>
</form>

